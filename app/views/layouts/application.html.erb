<!DOCTYPE html>
<html>
  <head>
    <title>My House</title>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= csrf_meta_tags %>
  </head>

  <body>
  <nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <div class="navbar-brand" href="#">My House</div>
    </div>
    <ul class="nav navbar-nav">
      <li><%= link_to 'Groups Page', groups_path %> </li>
      <li><%= link_to 'Events Page', events_path %> </li>
      <!-- <li class="active"><a href="#">Events</a>
      <!-- <li><a href="#">Page 1</a></li> -->
      <!-- <li><a href="#">Page 2</a></li>
      <li><a href="#">Page 3</a></li> -->
      <% if current_user %>
      <a>Welcome, <%= current_user.name %></a>
      <li> <%= link_to 'Log Out', logout_path, method: :delete %></li>
      <% else %>
      <li><%= link_to 'Log In', login_path %></li>
      <li><%= link_to 'Sign Up', new_user_path %></li>
      <% end %>
    </ul>
  </div>
</nav>
    <%= yield %>
<br>
  <div>
    <img src="http://i.imgur.com/jiCzlqy.png?3" alt=""></img>
 </div>
  </body>
</html>
